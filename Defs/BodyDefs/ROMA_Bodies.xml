    <BodyDef>
        <defName>ROMA_SpiderBody</defName>
        <label>spider-like</label>
        <corePart>
            <def>Shell</def>
            <height>Middle</height>
            <depth>Outside</depth>
            <parts>
                <li>
                    <def>Stomach</def>
                    <coverage>0.05</coverage>
                    <depth>Inside</depth>
                </li>
                <li>
                    <def>InsectHeart</def>
                    <coverage>0.03</coverage>
                    <depth>Inside</depth>
                </li>
                <li>
                    <def>ROMA_Cephalothorax</def>
                    <coverage>0.18</coverage>
                    <height>Top</height>
                    <parts>
                        <li>
                            <def>InsectHead</def>
                            <coverage>0.65</coverage>
                            <groups>
                                <li>HeadAttackTool</li>
                            </groups>
                            <parts>
                                <li>
                                    <def>Brain</def>
                                    <coverage>0.2</coverage>
                                    <depth>Inside</depth>
                                </li>
                                <li>
                                    <def>Eye</def>
                                    <customLabel>left anterior lateral eye</customLabel>
                                    <coverage>0.025</coverage>
                                </li>
                                <li>
                                    <def>Eye</def>
                                    <customLabel>right anterior lateral eye</customLabel>
                                    <coverage>0.025</coverage>
                                </li>
                                <li>
                                    <def>Eye</def>
                                    <customLabel>left anterior median eye</customLabel>
                                    <coverage>0.025</coverage>
                                </li>
                                <li>
                                    <def>Eye</def>
                                    <customLabel>right anterior median eye</customLabel>
                                    <coverage>0.025</coverage>
                                </li>
                                <li>
                                    <def>Eye</def>
                                    <customLabel>left posterior lateral eye</customLabel>
                                    <coverage>0.025</coverage>
                                </li>
                                <li>
                                    <def>Eye</def>
                                    <customLabel>right posterier lateral eye</customLabel>
                                    <coverage>0.025</coverage>
                                </li>
                                <li>
                                    <def>Eye</def>
                                    <customLabel>left posterior median eye</customLabel>
                                    <coverage>0.025</coverage>
                                </li>
                                <li>
                                    <def>Eye</def>
                                    <customLabel>right posterior median eye</customLabel>
                                    <coverage>0.025</coverage>
                                </li>
                                <li>
                                    <def>InsectMouth</def>
                                    <coverage>0.10</coverage>
                                    <groups>
                                        <li>Mouth</li>
                                    </groups>
                                </li>
                            </parts>
                        </li>
                    </parts>
                </li>
                <li>
                    <def>ROMA_Stinger</def>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                    <groups>
                        <li>ROMA_StingerGroup</li>
                    </groups>
                </li>
                <li>
                    <def>ROMA_LeftPedipalp</def>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                </li>
                <li>
                    <def>ROMA_RightPedipalp</def>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                </li>
                <li>
                    <def>InsectLeg</def>
                    <customLabel>front left leg</customLabel>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                    <groups>
                        <li>FrontLeftLeg</li>
                    </groups>
                </li>
                <li>
                    <def>InsectLeg</def>
                    <customLabel>front right leg</customLabel>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                    <groups>
                        <li>FrontRightLeg</li>
                    </groups>
                </li>
                <li>
                    <def>InsectLeg</def>
                    <customLabel>middle left leg</customLabel>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                </li>
                <li>
                    <def>InsectLeg</def>
                    <customLabel>middle right leg</customLabel>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                </li>
                <li>
                    <def>ROMA_SecondMiddleLeftInsectLeg</def>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                </li>
                <li>
                    <def>ROMA_SecondMiddleRightInsectLeg</def>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                </li>
                <li>
                    <def>InsectLeg</def>
                    <customLabel>rear left leg</customLabel>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                </li>
                <li>
                    <def>InsectLeg</def>
                    <customLabel>rear right leg</customLabel>
                    <coverage>0.06</coverage>
                    <height>Bottom</height>
                </li>
            </parts>
        </corePart>
